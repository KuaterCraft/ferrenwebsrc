          <script>
            if (location.pathname.endsWith('.html')) {
              var newPath = location.pathname.replace('.html', '');
              window.history.pushState({}, '', newPath);
            }
          </script>

          <script src="../js/bootstrap.bundle.min.js"></script>

          <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
          <script>
            const navbarToggler = document.getElementById('navbar-toggler');
            const bar2 = navbarToggler.querySelector('.bar2');
            navbarToggler.addEventListener('click', () => {
              bar2.style.opacity = 0;
            });
          </script>
          <script>
            const urlParams = new URLSearchParams(window.location.search);
            const username = urlParams.get('username');
            const avatarURL = urlParams.get('avatarURL');
            const guildData = JSON.parse(urlParams.get('guilds'));

            if (username && avatarURL) {
              localStorage.setItem('username', username);
              localStorage.setItem('avatarURL', avatarURL);
              localStorage.setItem('guildData', JSON.stringify(guildData));

              const expirationTime = new Date().getTime() + (24 * 60 * 60 * 1000); // Set expiration to one day from now
              localStorage.setItem('expirationTime', expirationTime);

              window.history.replaceState({}, document.title, '/');

              window.location.href = 'https://ferren.fr.to/';
            }

            const loginButton = document.getElementById('loginButton');
            const profilePicture = document.createElement('img');
            profilePicture.id = 'pfpDc';
            profilePicture.classList.add('profile-picture');
            const serversLink = document.getElementById('serversLink');
            const changeLogs = document.getElementById('changeLogs');
            const storedAvatarURL = localStorage.getItem('avatarURL');
            const storedGuildData = JSON.parse(localStorage.getItem('guildData'));
            const expirationTime = localStorage.getItem('expirationTime');

            if (storedAvatarURL) {
              loginButton.href = '';
              loginButton.innerText = 'Logout';
              profilePicture.src = storedAvatarURL;
              profilePicture.alt = 'Profile Picture';
            } else {
              serversLink.style.display = 'none';
              changeLogs.style.display = 'none';
              manageAcc.style.display = 'none';
              localStorage.clear(); // Clear any expired session data
            }

            profilePicture.addEventListener('click', () => {
              localStorage.clear();
              loginButton.innerText = 'Login';
              loginButton.href = 'https://ferren.fr.to';
              profilePicture.remove();
              alert('Logged out!');
              window.location.reload();
            });

            if (storedAvatarURL) {
              loginButton.prepend(profilePicture);
            }
          </script>
